<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="true" />
	<httpProtocol>  
     		<customHeaders>  
        		<add name="Access-Control-Allow-Origin" value="*" />  
        		<add name="Access-Control-Allow-Methods" value="POST" />  
